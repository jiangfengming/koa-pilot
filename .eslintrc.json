{
  "root": true,
  "parser": "@typescript-eslint/parser",

  "plugins": [
    "@typescript-eslint"
  ],

  "extends": [
    "unambiguous",
    "plugin:@typescript-eslint/recommended"
  ],

  "env": {
    "node": true
  },

  "rules": {
    "no-extra-parens": "off",
    "@typescript-eslint/no-extra-parens": "error",
    "@typescript-eslint/member-delimiter-style": "error"
  }
}
